datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  id        Int      @id @default(autoincrement())
  name String
  createdAt DateTime @default(now())
}

model User {
  id        Int      @id @default(autoincrement())
  username String
  nickname String
  createdAt DateTime @default(now())
}